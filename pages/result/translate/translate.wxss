
.section{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    background: #ffffff;
    margin-top: 10rpx;
}

.picker{
    font-size: 35rpx;
    margin: 25rpx 0rpx;
}

.picker.left{
    margin-left: 90rpx;
}

.icon image{
    width: 80rpx;
    height: 60rpx;
}

.picker.right{
    margin-right: 90rpx;
}

.output{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.output .icon{
    margin-top: 30rpx;
    height: 30rpx;
    width: 51.45rpx;
}

.output view image{
    height: 30rpx;
    width: 30rpx;
    margin-left: 10rpx;
    margin-bottom: 10rpx;
}

.body button{
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  width: 600rpx;
  color: #ffffff;
}